{
    "files.associations": {
        "*.env": "shellscript",
        "*.env.*": "shellscript",
    },
    "files.trimTrailingWhitespace": true,
    "debug.javascript.codelens.npmScripts": "never",
    "breadcrumbs.symbolPath": "off",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.colorDecorators": false,
    "editor.cursorStyle": "block",
    "editor.dragAndDrop": false,
    "editor.folding": false,
    "editor.fontSize": 16,
    "editor.glyphMargin": false,
    "editor.highlightActiveIndentGuide": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.lineNumbers": "off",
    "editor.matchBrackets": "near",
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "files.insertFinalNewline": true,
    "editor.quickSuggestions": {
        // "other": false,
        "comments": false,
        "strings": false,
    },
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.horizontalScrollbarSize": 0,
    "editor.scrollbar.vertical": "hidden",
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.wrappingIndent": "indent",
    "explorer.autoReveal": false,
    "explorer.compactFolders": false,
    "explorer.decorations.badges": false,
    "explorer.decorations.colors": false,
    "explorer.openEditors.visible": 0,
    "extensions.ignoreRecommendations": true,
    "files.autoSave": "onFocusChange",
    "scm.countBadge": "off",
    "scm.diffDecorationsGutterVisibility": "hover",
    "search.searchOnType": false,
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/*.min.*": true,
        "**/public": true,
        "package-lock.json": true,
    },
    "search.quickOpen.includeHistory": false,
    "window.customMenuBarAltFocus": false,
    "workbench.activityBar.visible": false,
    "workbench.commandPalette.history": 0,
    "editor.links": false,
    "editor.occurrencesHighlight": false,
    "editor.renderIndentGuides": false,
    "editor.selectionHighlight": true,
    "workbench.editor.highlightModifiedTabs": false,
    "workbench.editor.decorations.badges": false,
    "workbench.editor.decorations.colors": false,
    "workbench.editor.showIcons": false,
    "workbench.editor.tabCloseButton": "off",
    "workbench.iconTheme": null,
    "workbench.startupEditor": "none",
    "workbench.tips.enabled": false,
    "workbench.tree.indent": 16,
    "window.title": "${rootName}",
    "workbench.tree.renderIndentGuides": "none",
    "workbench.colorCustomizations": {
        "editor.findMatchBackground": "#fc9e2150",
        "editor.findMatchHighlightBackground": "#e5ccaf30",
        "editor.selectionHighlightBackground": "#ff000000",
        "editor.selectionHighlightBorder": "#fc9e21",
        "editor.inactiveSelectionBackground": "#e5ccaf10",
        "editorWarning.foreground": "#c84c61",
        "sideBarTitle.foreground": "#ffffff00",
        "sideBar.background": "#3b3a36",
        "sideBar.foreground": "#e5ccaf60",
        "sideBarSectionHeader.background": "#44413c",
        "sideBarSectionHeader.foreground": "#ffffff20",
        "sideBar.border": "#44413c",
        "editorSuggestWidget.selectedBackground": "#ffffff20",
        "quickInput.background": "#293134",
        "editorGroup.border": "#44413c",
        "editorGroupHeader.tabsBackground": "#3b3a36",
        "scrollbar.shadow": "#ffffff00",
        "tab.activeForeground": "#fc9e21",
        "tab.unfocusedActiveForeground": "#e5ccaf",
        "tab.hoverForeground": "#e5ccaf",
        "tab.border": "#3b3a36",
        "tab.inactiveBackground": "#3b3a36",
        "tab.inactiveForeground": "#ffffff30",
        "tab.unfocusedHoverForeground": "#e5ccaf",
        "list.focusBackground": "#ffffff20",
        "list.activeSelectionBackground": "#293134",
        "list.activeSelectionForeground": "#f5883b",
        "list.inactiveSelectionBackground": "#ffffff00",
        "list.inactiveSelectionForeground": "#f5883b",
        "list.hoverBackground": "#1f252760",
        "list.focusOutline": "#ffffff00",
        "list.inactiveFocusOutline": "#ffffff00",
        "editorBracketMatch.background": "#3b3a36",
        "editorBracketMatch.border": "#fc9e21",
        "editorWhitespace.foreground": "#3b3a3640",
        "editorUnnecessaryCode.border": "#ff0000",
        "editorUnnecessaryCode.opacity": "#00000080",
        "focusBorder": "#ffffff00",
        "statusBar.background": "#44413c",
        "statusBar.foreground": "#e5ccaf60",
        "editorCursor.background": "#1f2527",
        "editorCursor.foreground": "#ffffff",
        "editor.selectionBackground": "#e5ccaf30",
        "editor.lineHighlightBackground": "#44413c",
        "editor.lineHighlightBorder": "#44413c",
        "editor.background": "#3b3a36",
        "editor.foreground": "#e5ccaf",
        "foreground": "#e5ccaf",
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "name": "block punctuation",
                "scope": "punctuation.definition.block",
                "settings": {
                    "foreground": "#fc9e21",
                }
            },
            {
                "name": "comment",
                "scope": [
                    "comment",
                    "comment.block.preprocessor",
                    "comment punctuation",
                    "punctuation.definition.block.tag.jsdoc",
                    "storage.type.class.jsdoc",
                    "entity.name.type.instance.jsdoc",
                    "variable.other.jsdoc"
                ],
                "settings": {
                    "foreground": "#e5ccaf40"
                }
            },
            {
                "name": "import",
                "scope": [
                    "variable.other.readwrite.alias",
                    "constant.language.import-export-all"
                ],
                "settings": {
                    "foreground": "#bd8b69",
                }
            },
            {
                "name": "string",
                "scope": [
                    "string",
                    "meta.property-value.string",
                    "support.constant.property-value.string",
                    "constant.character.escape"
                ],
                "settings": {
                    "foreground": "#e5ccaf"
                }
            },
            {
                "name": "number",
                "scope": [
                    "constant.numeric",
                    "meta.property-value.numeric",
                    "support.constant.property-value.numeric",
                    "meta.property-value.color",
                    "support.constant.property-value.color"
                ],
                "settings": {
                    "foreground": "#e5ccaf"
                }
            },
            {
                "name": "constant",
                "scope": [
                    "constant.language",
                    "variable.other.constant",
                ],
                "settings": {
                    "foreground": "#e5ccaf"
                }
            },
            {
                "name": "variable",
                "scope": [
                    "variable",
                    "support.variable",
                    "variable.other.property",
                    "variable.other.enummember",
                    "variable.language.this",
                ],
                "settings": {
                    "foreground": "#e5ccaf",
                }
            },
            {
                "name": "punctuation",
                "scope": [
                    "keyword.operator",
                    "punctuation.support.type.property-name"
                ],
                "settings": {
                    "foreground": "#e5ccaf"
                }
            },
            {
                "name": "structural keyword",
                "scope": [
                    "keyword",
                    "meta.property-value.keyword",
                    "support.constant.property-value.keyword",
                    "keyword.control",
                    "storage.modifier.async",
                ],
                "settings": {
                    "foreground": "#fc9e21"
                }
            },
            {
                "name": "non structural keyword",
                "scope": [
                    "keyword.other",
                    "keyword.operator.new",
                    "keyword.operator.expression.typeof",
                    "keyword.operator.expression.import",
                    "keyword.operator.sizeof",
                    "variable.language.super",
                ],
                "settings": {
                    "foreground": "#e5ccaf",
                }
            },
            {
                "name": "scope modifier",
                "scope": [
                    "storage.modifier",
                    "storage.type",
                    "keyword.operator.expression.instanceof",
                    "keyword.operator.expression.of",
                    "keyword.operator.expression.in",
                    "keyword.control.as",
                ],
                "settings": {
                    "foreground": "#bd8b69"
                }
            },
            {
                "name": "arrow function symbol",
                "scope": "storage.type.function.arrow",
                "settings": {
                    "foreground": "#e5ccaf",
                }
            },
            {
                "name": "punctuation",
                "scope": [
                    "punctuation.definition.tag",
                    "punctuation.definition.template-expression.begin",
                    "punctuation.definition.template-expression.end",
                    "punctuation.separator",
                    "punctuation.section.arguments",
                ],
                "settings": {
                    "foreground": "#e5ccaf",
                }
            },
            {
                "name": "type",
                "scope": [
                    "meta.return-type",
                    "support.class",
                    "support.type",
                    "support.type.primitive",
                    "entity.name.type.interface",
                    "entity.name.type.class",
                    "entity.name.type",
                ],
                "settings": {
                    "foreground": "#bd8b6980",
                }
            },
            {
                "name": "entity",
                "scope": [
                    "entity.name.function",
                    "entity.name.type.enum",
                    "meta.function-call entity.name.function",
                    "entity.name.type.module",
                    "entity.name.function.preprocessor",
                    "meta.preprocessor",
                ],
                "settings": {
                    "foreground": "#e5ccaf",
                }
            },
            {
                "name": "Tag name",
                "scope": "entity.name.tag",
                "settings": {
                    "fontStyle": "",
                    "foreground": "#fc9e21"
                }
            },
            {
                "name": "Tag attribute",
                "scope": "entity.other.attribute-name",
                "settings": {
                    "foreground": "#bd8b69"
                }
            },
            {
                "name": "Invalid & deprecated",
                "scope": [
                    "invalid.deprecated",
                    "invalid"
                ],
                "settings": {
                    "foreground": "#c84c61",
                }
            },
            {
                "name": "JSON String",
                "scope": "meta.structure.dictionary.json string.quoted.double.json",
                "settings": {
                    "foreground": "#e5ccaf"
                }
            },
            {
                "name": "diff.header",
                "scope": ["meta.diff", "meta.diff.header"],
                "settings": {
                    "foreground": "#e5ccaf"
                }
            },
            {
                "name": "diff.deleted",
                "scope": "markup.deleted",
                "settings": {
                    "foreground": "#c84c61"
                }
            },
            {
                "name": "diff.inserted",
                "scope": "markup.inserted",
                "settings": {
                    "foreground": "#79b465"
                }
            },
            {
                "name": "diff.changed",
                "scope": "markup.changed",
                "settings": {
                    "foreground": "#72a8ba"
                }
            },
            {
                "name": "entity.name.filename.find-in-files",
                "scope": "entity.name.filename.find-in-files",
                "settings": {
                    "foreground": "#e5ccaf"
                }
            },
            {
                "name": "CSS value",
                "scope": ["meta.property-value", "support.constant.property-value"],
                "settings": {
                    "foreground": "#e5ccaf"
                }
            },
            {
                "name": "meta.structure.dictionary.json string.quoted.double.json",
                "scope": "meta.structure.dictionary.json string.quoted.double.json",
                "settings": {
                    "foreground": "#e5ccaf"
                }
            },
            {
                "name":
                    "meta.structure.dictionary.value.json string.quoted.double.json",
                "scope":
                    "meta.structure.dictionary.value.json string.quoted.double.json",
                "settings": {
                    "foreground": "#e5ccaf"
                }
            },
            {
                "name": "Markdown titles",
                "scope": [
                    "text.html.markdown heading",
                ],
                "settings": {
                    "foreground": "#fc9e21"
                }
            },
            {
                "name": "Markdown titles punctuation",
                "scope": [
                    "text.html.markdown punctuation.definition.heading",
                ],
                "settings": {
                    "foreground": "#e5ccaf40"
                }
            },
            {
                "name": "Markdown list",
                "scope": [
                    "punctuation.definition.list.begin.markdown",
                ],
                "settings": {
                    "foreground": "#fc9e21",
                    "fontStyle": ""
                }
            },
            {
                "name": "Markdown bold punctuation",
                "scope": [
                    "punctuation.definition.bold.markdown",
                ],
                "settings": {
                    "foreground": "#e5ccaf40",
                    "fontStyle": ""
                }
            },
            {
                "name": "Markdown bold text",
                "scope": [
                    "markup.bold.markdown",
                ],
                "settings": {
                    "foreground": "#e5ccaf",
                    "fontStyle": "bold"
                }
            },
            {
                "name": "Markdown italic punctuation",
                "scope": [
                    "punctuation.definition.italic.markdown",
                ],
                "settings": {
                    "foreground": "#e5ccaf40",
                    "fontStyle": ""
                }
            },
            {
                "name": "Markdown italic text",
                "scope": [
                    "markup.italic.markdown",
                ],
                "settings": {
                    "foreground": "#e5ccaf",
                    "fontStyle": "italic"
                }
            },
            {
                "name": "GitGutter deleted",
                "scope": "markup.deleted.git_gutter",
                "settings": {
                    "foreground": "#c84c61"
                }
            },
            {
                "name": "GitGutter inserted",
                "scope": "markup.inserted.git_gutter",
                "settings": {
                    "foreground": "#79b465"
                }
            },
            {
                "name": "GitGutter changed",
                "scope": "markup.changed.git_gutter",
                "settings": {
                    "foreground": "#72a8ba"
                }
            },
            {
                "name": "CSS Class",
                "scope": "entity.other.attribute-name.class.css",
                "settings": {
                    "foreground": "#e5ccaf"
                }
            },
            {
                "name": "Tag XML",
                "scope": ["tag.xml", "tag.html"],
                "settings": {
                    "foreground": "#e5ccaf"
                }
            },
            {
                "name": "Makefile Variables",
                "scope": ["variable.language.makefile", "variable.other.makefile"],
                "settings": {
                    "foreground": "#e5ccaf"
                }
            },
            {
                "name": "property name",
                "scope": [
                    "meta.property-name.css",
                    "support.type.property-name",
                ],
                "settings": {
                    "foreground": "#fc9e21"
                }
            },
            {
                "name": "javascript object literal",
                "scope": [
                    "meta.objectliteral punctuation.definition.block",
                    "meta.object.type punctuation.definition.block",
                ],
                "settings": {
                    "foreground": "#e5ccaf"
                }
            }
        ]
    },
    "zenMode.hideTabs": false,
    "breadcrumbs.enabled": false,
    "git.decorations.enabled": false,
    "search.collapseResults": "alwaysCollapse",
    "editor.emptySelectionClipboard": false,
    "editor.lightbulb.enabled": false,
    "editor.smoothScrolling": true,
    "git.enableStatusBarSync": false,
    "window.menuBarVisibility": "toggle",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "workbench.editor.untitled.hint": "hidden",
    "security.workspace.trust.untrustedFiles": "open",
    "liveshare.authenticationProvider": "GitHub",
}
